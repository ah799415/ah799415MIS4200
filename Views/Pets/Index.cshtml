            @Html.DisplayFor(modelItem => item.petFirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.petLastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.phone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.petSince)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.petID }) |
